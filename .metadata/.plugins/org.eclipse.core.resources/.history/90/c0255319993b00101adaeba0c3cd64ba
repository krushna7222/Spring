package com.di.injectingCollections;

import java.awt.List;
import java.util.Map;
import java.util.Set;

public class Emp {

	private String name;
	private ArrayList<long> phone;
	private Set<String> address;
	private Map<String, Integer> course;
	
	public Emp() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Emp(String name, List phone, Set<String> address, Map<String, Integer> course) {
		super();
		this.name = name;
		this.phone = phone;
		this.address = address;
		this.course = course;
	}

	public String getName() {
		return name;
	}

	public List getPhone() {
		return phone;
	}

	public Set<String> getAddress() {
		return address;
	}

	public Map<String, Integer> getCourse() {
		return course;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setPhone(List phone) {
		this.phone = phone;
	}

	public void setAddress(Set<String> address) {
		this.address = address;
	}

	public void setCourse(Map<String, Integer> course) {
		this.course = course;
	}

	@Override
	public String toString() {
		return "Emp [name=" + name + ", phone=" + phone + ", address=" + address + ", course=" + course + "]";
	}
	
	
	
}
